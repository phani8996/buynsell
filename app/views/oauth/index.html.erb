
    <%= link_to "sign in",  @auth_url, :id =>"login2" %>
    <%= link_to "Home",  products_path, :id =>"login1" %>

<%if session[:access_token] %>
	<%= @signout %>
<%end%>
<script type='text/javascript'>
$(function(){
    <% if !logged_in? %>
        window.location.href = $('#login2').attr('href');
    <% else %>
        window.location.href = $('#login1').attr('href');
    <% end %>
});
</script>